{
  "input": {
    "geotweets": "./../../dbs/sweden/geotweets.csv",
    "home_locations": "./../../dbs/sweden/homelocations.csv"
  },
  "model": {
    "p": 0.66,
    "gamma": 0.6,
    "region_sampling": {
      "name": "zipf",
      "s": 1.2
    },
    "jump_size_sampling": {
      "name": "trueProb"
    }
  },
  "sampler": {
    "daily_trips_sampling": {
      "name": "normal",
      "mean": 3.143905,
      "std": 1.880373
    },
    "num_days": 140
  },
  "validation": {
    "gravity": {
      "beta": 0.03
    },
    "spssim": {
      "quantiles": 100
    }
  }
}